ALTER TABLE TBL_PROJECT_PLANNING
 DROP PRIMARY KEY CASCADE;

DROP TABLE TBL_PROJECT_PLANNING CASCADE CONSTRAINTS;

CREATE TABLE TBL_PROJECT_PLANNING
(
  ID             INTEGER                        NOT NULL,
  NAME           VARCHAR2(100 CHAR),
  DESCRIPTION    VARCHAR2(2000 CHAR),
  CREATED_DATE   TIMESTAMP(6) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  MODIFIED_DATE  TIMESTAMP(6) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  CREATED_BY     VARCHAR2(100 CHAR),
  MODIFIED_BY    VARCHAR2(100 CHAR),
  ID_PROJECT     INTEGER
)
TABLESPACE SYSTEM
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


ALTER TABLE TBL_PROJECT_DOCUMENT
 DROP PRIMARY KEY CASCADE;

DROP TABLE TBL_PROJECT_DOCUMENT CASCADE CONSTRAINTS;

CREATE TABLE TBL_PROJECT_DOCUMENT
(
  ID                  INTEGER                   NOT NULL,
  ID_PROJECT          INTEGER,
  FILE_NAME           VARCHAR2(200 CHAR),
  HASH_CODE_URL       VARCHAR2(200 CHAR),
  CREATED_DATE        TIMESTAMP(6) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  MODIFIED_DATE       TIMESTAMP(6) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  CREATED_BY          VARCHAR2(100 CHAR),
  MODIFIED_BY         VARCHAR2(100 CHAR),
  ORIGINAL_FILE_NAME  VARCHAR2(200 CHAR)
)
TABLESPACE SYSTEM
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


ALTER TABLE TBL_ITEM_TASK_TIME
 DROP PRIMARY KEY CASCADE;

DROP TABLE TBL_ITEM_TASK_TIME CASCADE CONSTRAINTS;

CREATE TABLE TBL_ITEM_TASK_TIME
(
  ID             INTEGER                        NOT NULL,
  ID_ITEM_TASK   INTEGER,
  NO_WEEK        INTEGER,
  PERCENTAGE     FLOAT(126)                     DEFAULT 0,
  CREATED_DATE   TIMESTAMP(0) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  MODIFIED_DATE  TIMESTAMP(0) WITH LOCAL TIME ZONE DEFAULT SYSDATE,
  CREATED_BY     VARCHAR2(100 CHAR),
  MODIFIED_BY    VARCHAR2(100 CHAR),
  START_WEEK     TIMESTAMP(0) WITH LOCAL TIME ZONE,
  END_WEEK       TIMESTAMP(0) WITH LOCAL TIME ZONE
)
TABLESPACE SYSTEM
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
