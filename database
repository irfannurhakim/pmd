                FREELIST GROUPS  1
               ));

ALTER TABLE TBL_USER ADD (
  CONSTRAINT FK_TBL_USER_REFERENCE_TBL_USER 
 FOREIGN KEY (ID_USER_TYPE) 
 REFERENCES TBL_USER_TYPE (ID));

ALTER TABLE TBL_ITEM_TASK ADD (
  CONSTRAINT FK_TBL_ITEM_REFERENCE_TBL_PROJ 
